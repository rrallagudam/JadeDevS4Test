    <validationRules>
        <fullName>User_or_Queue_Route_Only</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
  NOT(ISBLANK(Queue_ID__c)),  
  NOT(ISBLANK(User__c))
)</errorConditionFormula>
        <errorMessage>Territory may route to only a User or Queue. Not both. Please delete the User or Queue assignment.</errorMessage>
    </validationRules>

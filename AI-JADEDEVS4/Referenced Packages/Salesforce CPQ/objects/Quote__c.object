    <fields>
        <fullName>DaysQuoteOpen__c</fullName>
        <deprecated>false</deprecated>
        <description>Number of days the quote has been opened.</description>
        <externalId>false</externalId>
        <formula>if(OR(ISPICKVAL( Status__c ,&quot;Draft&quot; ),ISPICKVAL( Status__c ,&quot;In Review&quot;),ISPICKVAL( Status__c , &quot;Approved&quot;), ISPICKVAL( Status__c , &quot;Presented&quot;)),Round(NOW() - CreatedDate,0) ,null)</formula>
        <inlineHelpText>Number of days the quote has been opened.</inlineHelpText>
        <label>Days Quote Open</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>

        <fullName>CustomerDiscount__c</fullName>
        <deprecated>false</deprecated>
        <description>Additional discount extended to customer.</description>
        <externalId>false</externalId>
        <inlineHelpText>Additional discount extended to the customer on this quote. You&apos;ll be able to override this value at individual line item level. Any line items that don&apos;t have additional discount explicitly specified will default to this value.</inlineHelpText>
        <label>Additional  Disc. (%)</label>
        <precision>5</precision>
        <required>false</required>

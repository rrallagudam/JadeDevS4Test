        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>AdditionalDiscountAmount__c</fullName>
        <deprecated>false</deprecated>
        <description>Amount of additional discount on this quote.</description>
        <externalId>false</externalId>
        <formula>RegularAmount__c - CustomerAmount__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Amount additionally discounted on this quote manually.</inlineHelpText>
        <label>Addl. Disc. Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>AverageCustomerDiscount__c</fullName>
        <deprecated>false</deprecated>
        <description>Average customer discount (in percent) for this quote.</description>
        <externalId>false</externalId>
        <formula>IF(ListAmount__c = 0, 0, ((ListAmount__c - CustomerAmount__c) / ListAmount__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Average customer discount (in percent) for this quote.</inlineHelpText>
        <label>Avg. Customer Disc. (%)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>AveragePartnerDiscount__c</fullName>
        <deprecated>false</deprecated>
        <description>Average partner discount (in percent) for this quote.</description>
        <externalId>false</externalId>
        <formula>IF(CustomerAmount__c = 0, 0, (CustomerAmount__c -  NetAmount__c) / CustomerAmount__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Average partner discount (in percent) for this quote.</inlineHelpText>
        <label>Avg. Partner Disc. (%)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>

    <validationRules>
        <fullName>End_Date_Before_Start_Date</fullName>
        <active>true</active>
        <description>Validates the End Date starting after the Start Date</description>
        <errorConditionFormula>IF( AND( NOT(ISNULL(EndDate__c)), EndDate__c &lt; StartDate__c), TRUE, null)</errorConditionFormula>
        <errorMessage>The End Date can not be after the Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_First_Segment_Term_End_Date</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(ISNULL(FirstSegmentTermEndDate__c)) &amp;&amp; NOT(ISNULL(StartDate__c)) &amp;&amp; (NOT(ISNULL(SubscriptionTerm__c)) || NOT(ISNULL(EndDate__c))) &amp;&amp;
(FirstSegmentTermEndDate__c &lt; StartDate__c || IF(NOT(ISNULL(EndDate__c)),FirstSegmentTermEndDate__c &gt; EndDate__c, IF(LOWER($Setup.GeneralSettings__c.SubscriptionTermUnit__c)=&apos;day&apos;, FirstSegmentTermEndDate__c &gt; StartDate__c + SubscriptionTerm__c-1, FirstSegmentTermEndDate__c &gt; StartDate__c + (SubscriptionTerm__c * 30.4375))))</errorConditionFormula>
        <errorDisplayField>FirstSegmentTermEndDate__c</errorDisplayField>
        <errorMessage>First Segment Term End Date must fall between Quote Start Date and Quote End Date</errorMessage>
    </validationRules>
